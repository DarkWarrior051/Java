import java.util.Scanner;
public class classwork4 {
	public static void main(String[] args) {
		 Scanner input = new Scanner(System.in);
		 System.out.print("Enter an integer: ");
		 int number = input.nextInt();
		 if(number%6==0) {
			 	System.out.println("The number is divisble by 2 and 3");
		 }
		 else if (number%2==0) {
			 System.out.println("The number is divisble by only 2.");
		 }
		 else if (number%3==0) {
			 System.out.println("The number is divisble by only 3.");
		 }
		 else 
			 System.out.println("The number is not divisbe by either.");
	
}
}
