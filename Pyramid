import java.util.Scanner;
public class pyramid {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.print("Enter an integer between 1-15:");
		int n = input.nextInt();
		 for (int i = 1; i <= n; i++) {
			   
			   for (int j = 1; j <= (n - i); j++) {
			    System.out.print("  ");
			   }
			    
			   
			 
			 }
			 
	}
	
}
